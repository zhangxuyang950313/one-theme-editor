    "@types/postcss-preset-env": "^6.7.2",
    "@types/react": "^17.0.3",
    "@types/react-color": "^3.0.4",
    "@types/react-dev-utils": "^9.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
