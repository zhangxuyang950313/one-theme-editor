    "xml-js": "^1.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,tsx}": [
      "npm run lint:eslint"
    ],
    "*.{css,less,scss}": [
      "npm run lint:style"
    ],
    "*.{json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  }
}
