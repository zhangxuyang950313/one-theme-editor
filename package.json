{
  "name": "electron-react-ts",
  "version": "1.0.0",
  "description": "Electron + React",
  "keywords": [],
  "author": {
    "name": "Sunny",
    "email": "240283142@qq.com"
  },
  "license": "ISC",
  "private": true,
  "main": "release.main/index.js",
  "scripts": {
    "build:webpack": "tsc --project webpack/tsconfig.json",
    "build:builder": "tsc --project scripts/tsconfig.json",
    "webpack:main:dev": "webpack --mode=development --config release.webpack/config.main.js",
    "webpack:main:pro": "webpack --mode=production  --config release.webpack/config.main.js",
    "webpack:render:pro": "webpack --mode=production --config release.webpack/config.renderer.js",
    "webpack:render:pro:analyze": "webpack --mode=production --config release.webpack/config.renderer.js  --analyze",
    "webpack:render:dev": "webpack-dev-server --mode=development --config release.webpack/config.renderer.js",
    "webpack:server:dev": "webpack --mode=development --config release.webpack/config.server.js",
    "webpack:server:pro": "webpack --mode=production  --config release.webpack/config.server.js",
    "build:main:dev": "npm run build:webpack && npm run webpack:main:dev",
    "build:main:pro": "npm run build:webpack && npm run webpack:main:pro",
    "build:render:dev": "npm run build:webpack && npm run webpack:render:dev",
    "build:render:pro": "npm run build:webpack && npm run webpack:render:pro",
    "build:server:dev": "npm run build:webpack && npm run webpack:server:dev",
    "build:server:pro": "npm run build:webpack && npm run webpack:server:pro",
    "start:electron": "npm run build:main:dev && electron ./release.main/index.js",
    "build:electron:win32": "npm run build:builder && electron-builder --win --ia32 -c release.scripts/electron-builder.js",
    "build:electron:win64": "npm run build:builder && electron-builder --win --x64 -c release.scripts/electron-builder.js",
    "build:electron:linux": "npm run build:builder && electron-builder --linux -c release.scripts/electron-builder.js",
    "build:electron:mac": "npm run build:builder && electron-builder --mac -c release.scripts/electron-builder.js",
    "build:electron": "npm run build:builder && electron-builder -c release.scripts/electron-builder.js",
    "build:win32": "npm run build:render:pro && npm run build:main:pro && npm run build:electron:win32",
    "build:win64": "npm run build:render:pro && npm run build:main:pro && npm run build:electron:win64",
    "build:linux": "npm run build:render:pro && npm run build:main:pro && npm run build:electron:linux",
    "build:mac": "npm run build:render:pro && npm run build:main:pro && npm run build:electron:mac",
    "build": "npm run build:render:pro && npm run build:main:pro && npm run build:electron",
    "start": "concurrently \"npm run build:render:dev\" \"wait-on http://localhost:3000 && npm run start:electron\"",
    "render:analyzer": "npm run build:webpack && npm run webpack:render:pro:analyze",
    "lint:eslint": "eslint --config .eslintrc.js --fix --ext .js,.jsx,.ts,.tsx,.less",
    "lint:style": "stylelint --config .stylelintrc.js **/*"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
