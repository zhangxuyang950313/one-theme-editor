    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
