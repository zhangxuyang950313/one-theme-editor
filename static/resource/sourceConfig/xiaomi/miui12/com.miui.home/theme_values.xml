<?xml version="1.0" encoding="utf-8"?>
<ThemeConfig>
	
	<!-- 桌面图标文字颜色 -->
	<color name="icon_title_text">${class_color_icon_title_text_light}</color>
	<color name="icon_title_text_dark">class_color_icon_title_text_dark</color>
	<color name="icon_title_text_shadow_light">#0d000000</color>
	<color name="icon_title_text_shadow">#0d000000</color>
	<!-- 桌面图标角标通知数字颜色，如：未读短信、未接电话、软件更新等数字 -->
	<color name="icon_message_text">class_color_icon_message_text</color>
	
	<!-- 图标文字大小 -->
	<!-- <dimen name="workspace_icon_text_size">class_dimen_workspace_icon_text_size</dimen> -->
	
	<!-- 文件夹缩略图宽度 -->
	<dimen name="folder_preview_width">class_dimen_folder_preview_width</dimen>
	<!-- 文件夹缩略图高度 -->
	<dimen name="folder_preview_height">class_dimen_folder_preview_height</dimen>
	<!-- 文件夹缩略图中图标的间隔 -->
	<dimen name="folder_preview_item_padding">class_dimen_folder_preview_item_padding</dimen>
	
	<!-- 编辑模式	图标多选	复选框 已选背景 -->
	<color name="checkbox_btn_on_background_color_light" package="miui">class_color_checkbox_btn_on</color>
	
<!-- 桌面底部搜索框 -->
	<!-- 长按搜索框后屏幕上层黑色遮罩 -->
	<!-- <color name="search_bar_settings_bg_color">#a5000000</color> -->
	<!-- 长按搜索框：弹窗内文字“设置” -->
	<!-- <color name="search_bar_settings_text_color">#a5000000</color> -->
	<!-- （背景颜色，默认#D9FFFFFF） -->
	<color name="search_bar_bg_color_dark">class_color_search_bg_dark_bg</color>
	<color name="search_bar_bg_color_light">class_color_search_bg_light_bg</color>
	<!-- （color：点击时波纹颜色，默认#11000000） -->
	<!-- <color name="search_bar_fd_color_dark">class_color_search_fd_dark</color> -->
	<!-- <color name="search_bar_fd_color_light">class_color_search_fd_light</color> -->
	<!-- （color：边框颜色，默认：#14000000） -->
	<color name="search_bar_stroke_color_dark">class_color_search_bg_dark_stroke</color>
	<color name="search_bar_stroke_color_light">class_color_search_bg_light_stroke</color>
	<!-- （dimens：边框大小，默认1dp） -->
	<!-- <dimen name="search_bar_stroke_width_dark">1.000000dp</dimen> -->
	<!-- <dimen name="search_bar_stroke_width_light">1.000000dp</dimen> -->

	
	<!-- app名称、内存信息 -->
	<color name="task_manager_text">class_color_task_manager_app_name</color>
	<color name="txt_memory_info_color">class_color_task_manager_app_name</color>
	<!-- 提示文字：近期没有任何内容 -->
	<color name="task_manager_no_recents_text">class_color_task_manager_recents_tips</color>
	<!-- 任务管理器 小窗应用 按钮背景、按钮文字 -->
	<color name="recent_tv_small_window_background_color">class_color_task_manager_window_bg</color>
	<color name="recents_tv_small_window_text_color">class_color_task_manager_window_text</color>
