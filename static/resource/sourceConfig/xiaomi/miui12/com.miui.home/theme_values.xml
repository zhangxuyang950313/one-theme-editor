	<color name="recent_menu_bg_color">class_color_task_manager_recent_menu_bg</color>
	<!-- 任务应用 菜单背景 二态 -->
	<!-- <color name="miuix_folme_color_touch_tint">class_color_taskwindow_bgp_color</color> -->
	<!-- √ 清理动画 -->
	<color name="recent_tick_anim_color">class_color_task_manager_tick_anim</color>
	
