<?xml version="1.0" encoding="utf-8"?>

<config version="1.0.0" description="桌面壁纸和首屏图标" screenWidth="1080"/>

<preview src="default_wallpaper.jpg"/>

<!-- 模板文件 -->
<template src="theme_values.xml" to="wallpaper/theme_values.xml"/>

<!-- 值定义文件 -->
<values src="source_config.xml"/>

<!-- 默认移动的文件 -->
<copy>
    <file from="./theme_fallback.xml">
        <to src="wallpaper/theme_fallback.xml"/>
    </file>
</copy>

