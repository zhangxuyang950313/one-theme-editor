    src="../icons/res/com.android.contacts.activities.TwelveKeyDialer.png"
>
    <to src="icons/res/com.android.contacts.activities.TwelveKeyDialer.png"/>
</image>

<!-- 文字 -->
<text name="图标文字" text="相册" 
    x="415" y="1250" size="36" align="center" alignV="center" 
    colorClass="action_bar_title_text_color_light"
