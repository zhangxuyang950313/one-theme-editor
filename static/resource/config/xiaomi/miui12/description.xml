<?xml version='1.0' encoding='utf-8' standalone='yes' ?>

<Root name="深色 MIUI12" version="1.0">

    <!-- 预览静态图 -->
    <Preview src="preview/preview.jpg"/>

    <!-- 适配UI版本描述 -->
    <UiVersion name="MIUI 12" code="10"/>

    <!-- 
        资源类型定义 
        type: 决定使用哪种编辑器
        image   - 图片编辑器
        color   - 颜色选择器
        boolean - 布尔选择器
        string  - 字符串输入
        number  - 数字输入
    -->
    <Resource name="图片"     protocol="file" type="image"/>
    <Resource name="颜色值"   protocol="xml"   type="color"  format="argb"/>
    <Resource name="布尔值"   protocol="xml"   type="boolean"/>
    <Resource name="尺寸值"   protocol="xml"   type="string"/>
    <Resource name="整型值"   protocol="xml"   type="number"/>

