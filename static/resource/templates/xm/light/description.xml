<?xml version='1.0' encoding='utf-8' standalone='yes' ?>
<description name="浅色模板" version="1.0"/>
<!-- 预览静态图 -->
<poster src="v12/wallpaper/default_wallpaper.jpg"/>
<!-- UI版本 -->
<uiVersion name="MIUI 11" src="v11"/>
<uiVersion name="MIUI 12" src="v12"/>
<!-- 壁纸 -->
<module name="图标和壁纸" icon="v12/icons/res/com.android.thememanager.png">
    <!-- 桌面壁纸 -->
    <class name="桌面壁纸和图标">
        <page src="wallpaper/desktop0.xml"/>
        <page src="wallpaper/desktop1.xml"/>
    </class>
    <!-- 锁屏壁纸 -->
    <class name="锁屏壁纸">
        <page src="wallpaper/lock.xml"/>
    </class>
    <class name="第三方图标">
        <page src="icons/pages/icons-more.xml"/>
    </class>
</module>
<module name="拨号与联系人" icon="v12/icons/res/com.android.contacts.activities.TwelveKeyDialer.png">
    <class name="联系人">
        <page src="com.android.contacts/pages/page1.xml"/>
        <!-- <page>com.android.contacts/pages/page2.xml</page> -->
    </class>
    <class name="拨号盘">
        <!-- <page>com.android.contacts/pages/page3.xml</page> -->
        <page src="com.android.contacts/pages/page4.xml"/>
    </class>
</module>
<module name="短信" icon="v12/icons/res/com.android.mms.png">
    <class name="短信主页面">
        <page src="com.android.mms/pages/page1.xml"/>
        <page src="com.android.mms/pages/page2.xml"/>
    </class>
</module>
<module name="设置" icon="v12/icons/res/com.android.settings.png">
    <class name="设置第一页">
        <page src="com.android.settings/pages/page1.xml"/>
        <page src="com.android.settings/pages/page2.xml"/>
    </class>
</module>
<module name="通知栏" icon="v12/icons/res/com.android.systemui.png">
    <class name="通知栏1">
        <page src="com.android.systemui/pages/page1.xml"/>
        <page src="com.android.systemui/pages/page2.xml"/>
    </class>
    <class name="通知栏-聚合通知">
        <page src="com.miui.notification/pages/page1.xml"/>
        <page src="com.miui.notification/pages/page2.xml"/>
    </class>
</module>
<module name="桌面" icon="v12/icons/res/com.android.system.notification_phone.png">
    <class name="桌面小部件">
        <page src="com.miui.home/pages/page1.xml"/>
        <page src="com.miui.home/pages/page2.xml"/>
    </class>
</module>
<module name="安全中心" icon="v12/icons/res/com.miui.securitycenter.png">
    <class name="安全中心">
        <page src="com.miui.securitycenter/pages/page1.xml"/>
        <page src="com.miui.securitycenter/pages/page2.xml"/>
    </class>
</module>
<module name="原生全局" icon="v12/icons/res/android.png">
    <class name="原生全局">
        <page src="framework-res/pages/page1.xml"/>
        <page src="framework-res/pages/page2.xml"/>
    </class>
</module>
